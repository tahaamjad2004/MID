class MusicLibrary:
    def __init__(self):
        pass
    class Song:
        def __init__(self,title,artist,album,duration):
            self.title=title
            self.artist=artist
            self.album=album
            self.duration=duration
    
    class PlayList:
        def __init__(self,song,title,description,list_of_songs):
            self.song=song
            self.title=title
            self.description=description
            self.list_of_songs=list_of_songs

    class Library:
        def __init__(self,list_of_playlist_objects):
            self.list_of_playlist_objects=list_of_playlist_objects
